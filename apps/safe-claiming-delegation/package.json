{
  "name": "safe-claiming-delegation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next dev",
    "lint": "tsc && next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier -w \"{src,pages,scripts}/**/*.{ts,tsx,css,js}\"",
    "compile": "yarn hardhat compile",
    "generate-types:safe-token": "typechain --target ethers-v5 --out-dir ./src/types/contracts/safe-token './artifacts/safe-token/contracts/**/*[!.dbg].json'",
    "generate-types:delegate-registry": "typechain --target ethers-v5 --out-dir ./src/types/contracts/delegate-registry './artifacts/delegate-registry/contracts/**/*[!.dbg].json'",
    "generate-types": "yarn generate-types:safe-token && yarn generate-types:delegate-registry",
    "postinstall": "yarn hardhat compile && yarn run generate-types",
    "routes": "node scripts/generate-routes.js > src/config/routes.ts && prettier -w src/config/routes.ts",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "deploy:s3": "bash ../../scripts/deploy_to_s3_bucket.sh",
    "deploy:pr": "bash ../../scripts/deploy_pr.sh",
    "deploy:prod-hook": "bash ../../scripts/prepare_production_deployment.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@gnosis.pm/safe-apps-provider": "^0.15.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "@safe-global/safe-gateway-typescript-sdk": "^3.5.5",
    "@tanstack/react-query": "^4.22.0",
    "@web3-onboard/coinbase": "^2.2.0",
    "@web3-onboard/core": "2.8.5",
    "@web3-onboard/injected-wallets": "^2.4.0",
    "@web3-onboard/keystone": "^2.3.2",
    "@web3-onboard/ledger": "^2.3.2",
    "@web3-onboard/tallyho": "^2.0.1",
    "@web3-onboard/trezor": "^2.3.2",
    "@web3-onboard/walletconnect": "^2.2.2",
    "bezier-easing": "^2.1.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.7.2",
    "next": "12.3.0",
    "use-sync-external-store": "1.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/use-sync-external-store": "^0.0.3",
    "delegate-registry": "gnosis/delegate-registry",
    "eslint-config-next": "13.1.1",
    "hardhat": "^2.12.5",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "safe-token": "safe-global/safe-token",
    "typechain": "^8.1.1"
  }
}
